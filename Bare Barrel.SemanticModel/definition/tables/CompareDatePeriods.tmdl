	partition CompareDatePeriods = calculated
		mode: import
		source =
				
				UNION (
				    ADDCOLUMNS (
				        DATESINPERIOD (
				            'Dim Date'[Date],
				            MAX ( 'Dim Date'[Date] ),
				            -7,
				            DAY
				        ),
				        -- Get the date range for the previous 7 days for sales calc representation in the bar chart.
				        "Type", "D-1",
				        "Sort", 1 -- Add a column to sort the periods in ascending order
				    ),
				    ADDCOLUMNS (
				        DATESINPERIOD (
				            'Dim Date'[Date],
				            MAX ( 'Dim Date'[Date] ) ,
				            -15,
				            DAY
				        ),
				        -- Get the date range for the previous 15 days
				        "Type", "Past 15 Days",
				        "Sort", 2
				    ),
				    ADDCOLUMNS (
				        DATESINPERIOD (
				            'Dim Date'[Date],
				            MAX ( 'Dim Date'[Date] ) ,
				            -28,
				            DAY
				        ),
				        -- Get the date range for the past 4 weeks
				        "Type", "Past 4 Weeks",
				        "Sort", 3
				    ),
				    ADDCOLUMNS (
				        DATESINPERIOD (
				            'Dim Date'[Date],
				            MAX ( 'Dim Date'[Date] ) ,
				            -90,
				            DAY
				        ),
				        -- Get the date range for the past 3 months
				        "Type", "Past 3 Months",
				        "Sort", 4
				    )
				)

